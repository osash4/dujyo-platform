{
  "name": "dujyo-entertainment",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite --port 5173",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start:backend": "cd ../dujyo-backend && npm run dev",
    "start:production": "serve -s dist -l 5173",
    "docker:build": "docker build -f Dockerfile.production -t dujyo-frontend:latest .",
    "docker:run": "docker run -p 5173:5173 dujyo-frontend:latest",
    "production": "npm run build && npm run start:production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test && npm run test:e2e",
    "test:artist": "playwright test tests/e2e/artist-flow.spec.ts",
    "test:user": "playwright test tests/e2e/user-flow.spec.ts"
  },
  "dependencies": {
